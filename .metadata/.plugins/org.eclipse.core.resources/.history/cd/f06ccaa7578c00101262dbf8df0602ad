package classes;

public class StudentTest {
	public static void main(String[] args) {
		//클래스, 인스턴
		Student st1 = new Student();
			
		//값입력
		st1.studentId = 1001;
		st1.name = "이수환";
		st1.grade = 1;
		
		//출력
		System.out.println("학번 : " + st1.studentId);
		System.out.println("이름 : " + st1.name);
		System.out.println("학년 : " + st1.grade);
	}

}
