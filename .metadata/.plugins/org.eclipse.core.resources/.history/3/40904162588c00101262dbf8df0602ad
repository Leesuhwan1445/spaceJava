package classes;

public class Student {
   //맴버 변수
   int studentId;//학번
   String name; //이름
   int grade; //학년
   

   //기본생성자 - 컴파일러가 자동생성해줌
   //Student(){}
   
  Student(int studentId, String name, int grade){
	  this.studentId = studentId;
	  this.name = name;
	  this.grade = grade;
	  
  }
   
   //학생 정보 출력 메서드
   void displayInfo() {
		System.out.println("학번 : " + studentId);
		System.out.println("이름 : " + name);
		System.out.println("학년 : " + grade);
	   
   }
}
