package classes;

//은행계좌 클래스를 정의
public class BankAccount {
	private String ano; //계좌정의
	private String owner; //예금주
	private int balance; //잔액
	
	
	//setter 메서드(설정자) - set+멤버변수()
	
	
	public void setAno(String ano ,String owner, int balance) {
		
		this.ano = ano;
		
		
	}
		
	//getter (접근자) - get + 멤버변수()
	
	public String getAno() {
		return ano;
	}

	

}
